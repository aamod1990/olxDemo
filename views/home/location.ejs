    <th>Id</th>
    <th>State</th>
    <th>City</th>
    <th>Location</th>
    <th>Action</th>
  </tr>
  <tr class="success">
    <td>1</td>
    <td>mp</td>
    <td>Indore</td>
    <td>Vijay_Nagar</td>
    <td><span><a href="#">Edit</a></span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span><a href="#">Delete</a></span></td>
  </tr>
  <tr  class="warning">
    <td>2</td>
    <td>mp</td>
    <td>Indore</td>
    <td>Vijay_Nagar</td>
    <td><span><a href="#">Edit</a></span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span><a href="#">Delete</a></span></td>
  </tr>
  <tr class="danger">
    <td>3</td>
    <td>mp</td>
    <td>Indore</td>
    <td>Vijay_Nagar</td>
    <td><span><a href="#">Edit</a></span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span><a href="#">Delete</a></span></td>
  </tr>
  <tr class="info">
   <td>4</td>
    <td>mp</td>
