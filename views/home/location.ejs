</tr>
</table>
</div>
<div class="clear"></div>
</div>
</div>
<div id="createstate_dialog">
 <form action="/createloc" method="post" name="createloc"  onsubmit="return(validate())">
  <fieldset>
   <span for="name">Enter State</span>
   <input type="text" name="state"  id="state" class="text ui-widget-content ui-corner-all"><br>
   <span for="name">Enter City</span>
   <input type="text" name="city"  id="city" class="text ui-widget-content ui-corner-all"><br>
   <span for="name">Enter Location</span>
   <input type="text" name="location"  id="location" class="text ui-widget-content ui-corner-all">
   <button type="submit" class="ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only" role="button"><span class="ui-button-text">submit</span></button>
  </fieldset>
 </form>
</div>
<script>
 $( "#createstate_dialog" ).dialog({
   autoOpen: false,
   modal : true,
   show: {
   effect: "blind",
   duration: 1000
  },
   hide: {
   effect: "explode",
   duration: 1000
  }
 });

  $(function() {    
   $("#addState").click(function(){
    $("#createstate_dialog").dialog('open');
   });   
  });
</script>
<script>
function validate () {
  if (document.createloc.state.value=="") {
       alert("Please Enter State Fast");
       document.createloc.state.focus();
       return false;
  };
   if (document.createloc.city.value=="") {
       alert("Please Enter City Fast");
       document.createloc.state.focus();
       return false;
  };
   if (document.createloc.location.value=="") {
       alert("Please Enter Location Fast");
       document.createloc.state.focus();
       return false;
  };
 return(true);
  }
</script>

<!-- Container-box-End -->
</body>

